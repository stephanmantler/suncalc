{
  "name": "suncalc-luxon",
  "version": "1.9.1",
  "type": "module",
  "description": "A tiny JavaScript library for calculating sun/moon positions and phases, integrated with Luxon for time zone support.",
  "homepage": "https://github.com/stephanmantler/suncalc",
  "keywords": [
    "sun",
    "astronomy",
    "math",
    "calculation",
    "sunrise",
    "sunset",
    "twilight",
    "moon",
    "illumination"
  ],
  "author": "Stephan Mantler",
  "repository": {
    "type": "git",
    "url": "git://github.com/stephanmantler/suncalc.git"
  },
  "main": "suncalc.js",
  "devDependencies": {
    "@types/luxon": "^3.4.2",
    "eslint": "^8.8.0",
    "eslint-config-mourner": "^2.0.1",
    "tape": "^5.5.3"
  },
  "eslintConfig": {
    "extends": "mourner",
    "rules": {
      "indent": 0,
      "array-bracket-spacing": 0,
      "strict": 0,
      "brace-style": 0
    },
    "env": {
      "amd": true
    }
  },
  "files": [
    "suncalc.js"
  ],
  "scripts": {
    "pretest": "eslint suncalc.js test.js",
    "test": "node test.js",
    "prepublishOnly": "npm test"
  },
  "jshintConfig": {
    "quotmark": "single",
    "trailing": true,
    "unused": true
  },
  "dependencies": {
    "luxon": "^3.4.4"
  }
}
